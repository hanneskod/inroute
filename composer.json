{
    "name": "hanneskod/inroute",
    "description": "Generate web router and controller dispatcher from docblock annotations",
    "keywords": ["Annotations", "Router", "Code generator"],
    "homepage": "https://github.com/hanneskod/inroute",
    "type": "library",
    "license": "WTFPL",
    "authors": [
        {
            "name": "Hannes ForsgÃ¥rd",
            "email": "hannes.forsgard@fripost.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "inroute\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "inroute\\example\\": "example/"
        }
    },
    "require": {
        "php": ">=5.5",
        "hanneskod/classtools": "dev-master@dev",
